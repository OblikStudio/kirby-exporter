{
  "name": "oblik/kirby-outsource",
  "description": "Kirby plugin for exporting and importing site content.",
  "type": "kirby-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Hristiyan Dodov",
      "email": "h.dodov@gmail.com",
      "homepage": "https://dodov.me/"
    }
  ],
  "scripts": {
    "test-run": "phpunit",
    "test-clean": "for /R %i in (*.bg.txt) do (rm \"%i\")",
    "test": [
      "@test-clean",
      "@test-run"
    ]
  },
  "require": {
    "getkirby/composer-installer": "^1.1",
    "league/html-to-markdown": "^4.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^8"
  },
  "autoload": {
    "psr-4": {
      "Oblik\\Outsource\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Oblik\\Outsource\\": "tests/classes"
    }
  },
  "extra": {
    "installer-name": "outsource"
  }
}
